{
  "kind": "collectionType",
  "collectionName": "user_slots",
  "info": {
    "singularName": "user-slot",
    "pluralName": "user-slots",
    "displayName": "user_slot",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "booked",
        "completed",
        "Didn't show up"
      ],
      "default": "booked"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "user_slots"
    },
    "payableAmount": {
      "type": "biginteger"
    },
    "services": {
      "type": "json"
    },
    "start": {
      "type": "time"
    },
    "end": {
      "type": "time"
    },
    "date": {
      "type": "date"
    },
    "isReviewed": {
      "type": "boolean",
      "default": false
    },
    "details": {
      "type": "json"
    },
    "invoice": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::invoice.invoice",
      "mappedBy": "userSlot"
    }
  }
}
